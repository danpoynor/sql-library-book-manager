  "dependencies": {
    "async": "^3.2.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "luxon": "^2.3.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "pug": "^3.0.2",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "npm:@vscode/sqlite3@5.0.7"
  }
}
